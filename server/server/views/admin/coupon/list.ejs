<% include ../header %>
    <!-- 路径导航 -->
    <ol class="breadcrumb">
        <li>
            <a href="#">栏目管理</a>
        </li>
        <li class="active">添加列表</li>
    </ol>
    <!-- 路径导航 END -->
    <table class="table table-hover">
        <tr>
            
            <td>优惠卷名称</td>
            <td>优惠券金额</td>
            <td>满多少用</td>
            <td>开始</td>
            <td>结束</td>
            <td>操作</td>
        </tr>
        <% data.forEach(function(val){ %>
            <tr>
                <td><%=val.coupon_name%></td>
                <td><%=val.coupon_fee%></td>
                <td><%=val.total_fee%></td>
                <td><%=new Date(val.from_time).toLocaleString()%></td>
                <td>
                    <%=new Date(val.from_time).toLocaleString()%>
                </td>
                <td>
                    <a href="/admin/category/edit/<%=val.id%>">编辑</a>
                    |
                    <a href="/admin/category/del/<%=val.id%>" onclick="return confirm('您确定要删除吗？')">删除</a>
                </td>
            </tr>
        <% })%>
    </table>
<% include ../footer %>
