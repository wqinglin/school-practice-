<div class="container swiperAdd">
   <div class="row">
       <form action="/admin/swiper/doadd" method="POST" enctype="multipart/form-data">
       <div class="col-md-6">
            <div class="input-group">
                <span class="input-group-addon">轮播图标题</span>
                <input type="text" class="form-control" name="title">
            </div>
       </div>
       <div class="col-md-6">
            <div class="input-group">
                <span class="input-group-addon">轮播图连接</span>
                <input type="text" class="form-control" name="url">
            </div>
       </div>
       <div class="col-md-6">
            <div class="input-group">
                <span class="input-group-addon">轮播图图像 </span>
                <input type="file" class="form-control" name="imgurl">
            </div>
       </div>
       <div class="col-md-12 content">
           
       </div>
       <div class="col-md-6">
            <input class="reset" type="reset">
       </div>
       <div class="col-md-6">
            <input class="submit" type="submit">
       </div>
